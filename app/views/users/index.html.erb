<div class="center">
  <div class="row">
    <% @users.each do |user| %>
       <div class="col-md-3">
          <div class="thumbnail">
            <%= link_to image_tag(user.image.url :thumb), user %>
            <div class="caption">
              <h3><%= user.name %></h3>
              <p><%= user.email %></p>
            </div>
          </div>
      </div>
    <% end %>
  </div>
</div>